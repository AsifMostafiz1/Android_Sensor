<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sensorproject">

    <uses-permission android:name="android.hardware.senson.proximity"
        android:required="true"/>

    <uses-permission android:name="android.hardware.senson.light"
        android:required="true"/>

    <uses-permission android:name="android.hardware.senson.accumulator"
        android:required="true"/>

    <uses-permission android:name="android.hardware.senson.gyroscope"
        android:required="true"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SensorProject">
        <activity android:name=".AllSensorValueActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>